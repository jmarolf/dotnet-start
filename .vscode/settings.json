        "*.globalconfig": "ini",
        "*.manifest": "xml",
        "*.nuspec": "xml",
        "*.pkgdef": "ini",
        "*.projitems": "msbuild",
        "*.props": "msbuild",
        "*.resx": "xml",
        "*.rsp": "Powershell",
        "*.shproj": "msbuild",
        "*.slnf": "json",
        "*.targets": "msbuild",
        "*.vbproj": "msbuild",
        "*.vsixmanifest": "xml",
